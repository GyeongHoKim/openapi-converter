{
	"name": "@gyeonghokim/openapi-converter",
	"version": "1.0.1",
	"private": false,
	"description": "Convert API collection files (Postman, Apidog, Bruno) to OpenAPI v3 format",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"openapi-converter": "dist/cli.js",
		"oac": "dist/cli.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GyeongHoKim/openapi-converter.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --write .",
		"prepare": "husky",
		"release": "semantic-release"
	},
	"keywords": [
		"openapi",
		"postman",
		"apidog",
		"bruno",
		"converter",
		"cli"
	],
	"author": "GyeongHo Kim",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@apidevtools/swagger-parser": "^12.1.0",
		"@biomejs/biome": "^2.3.8",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.2",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "^4.0.15",
		"@vitest/ui": "^4.0.15",
		"biome": "^0.3.3",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"semantic-release": "^25.0.2",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"lint-staged": {
		"*.{ts,js,json}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"@gyeonghokim/bruno-to-openapi": "^1.1.0",
		"js-yaml": "^4.1.1",
		"meow": "^14.0.0",
		"postman2openapi": "^1.2.1",
		"zod": "^4.1.13"
	}
}
